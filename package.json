{
  "name": "wallet-lib",
  "version": "7.21.8",
  "description": "Light wallet library for Dash",
  "main": "src/index.js",
  "unpkg": "dist/wallet-lib.min.js",
  "scripts": {
    "build:web": "webpack --stats-error-details",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "nyc yarn mocha 'src/**/*.spec.js'",
    "test:functional": "nyc yarn run mocha 'tests/functional/*.js'",
    "test:integration": "nyc yarn run mocha 'tests/integration/**/*.spec.js'",
    "test:browsers": "karma start ./karma.conf.js --single-run",
    "test": "yarn run test:unit && yarn run test:integration && yarn run test:functional && yarn run test:browsers",
    "prepublishOnly": "yarn run build:web",
    "postinstall": "rn-nodeify --install fs,buffer,path,crypto,stream,events,vm,assert,url --hack"
  },
  "ultra": {
    "concurrent": [
      "test"
    ]
  },
  "files": [
    "dist",
    "docs",
    "examples",
    "src"
  ],
  "dependencies": {
    "@dashevo/dashcore-lib": "~0.19.26",
    "@dashevo/dpp": "~0.21.0-dev.8",
    "@dashevo/grpc-common": "~0.5.4",
    "cbor": "^8.0.0",
    "crypto-js": "^4.0.0",
    "dapi-client": "git@github.com:DavitSo/js-dapi-client.git#v1.5.8-dev",
    "lodash": "^4.17.19",
    "path-browserify": "^1.0.1",
    "pbkdf2": "^3.1.1",
    "react-native-crypto": "^2.2.0",
    "react-native-level-fs": "^3.0.1",
    "react-native-randombytes": "^3.0.0",
    "readable-stream": "^1.0.33",
    "setimmediate": "^1.0.5",
    "stream-browserify": "^3.0.0",
    "vm-browserify": "^1.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "assert": "^1.1.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^4.9.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "crypto-browserify": "^3.12.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "events": "^1.0.0",
    "https-browserify": "^1.0.0",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0",
    "localforage": "^1.7.3",
    "mocha": "^9.1.2",
    "node-inspect-extracted": "^1.0.8",
    "nyc": "^15.1.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "rn-nodeify": "github:tradle/rn-nodeify",
    "sinon": "^11.1.2",
    "stream-http": "^3.2.0",
    "string_decoder": "^1.3.0",
    "url": "^0.10.3",
    "util": "^0.12.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1"
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
